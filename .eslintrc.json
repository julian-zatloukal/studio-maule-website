{
    "env": {
        "browser": true,
        "es6": true,
        "commonjs": true,
        "jquery": true,
        "node": true,
        "amd": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars":"warn"
    }
}